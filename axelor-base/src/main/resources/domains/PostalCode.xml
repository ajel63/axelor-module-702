<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.3.xsd">

  <module name="base" package="com.axelor.apps.base.db"/>

  <entity name="PostalCode" lang="java">

    <string name="code" title="Postal Code" namecolumn="true" required="true"/>
    <string name="areaCode" title="Area Code"/>
    <many-to-one name="city" ref="com.axelor.apps.base.db.City" title="City"/>
    <many-to-one name="country" ref="com.axelor.apps.base.db.Country" title="Country"/>
    <many-to-one name="county" ref="com.axelor.apps.base.db.Department" title="County"/>
    <many-to-one name="state" ref="com.axelor.apps.base.db.Region" title="State"/>

  </entity>

</domain-models>