<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="customer-ddia-line-grid" title="Customer DDIA"
    model="com.axelor.apps.base.db.CustomerDdiaLine" editable="true">
    <field name="startDate"/>
    <field name="endDate"/>
    <field name="redemptionCode"/>
    <field name="groupName"/>
    <field name="groupNumber"/>
    <field name="subscriberId"/>
  </grid>

  <form name="customer-ddia-line-form" title="Customer DDIA"
    model="com.axelor.apps.base.db.CustomerDdiaLine">
    <panel name="mainPanel">
      <field name="startDate"/>
      <field name="endDate"/>
      <field name="redemptionCode"/>
      <field name="groupName"/>
      <field name="groupNumber"/>
      <field name="subscriberId"/>
    </panel>
  </form>

</object-views>
