<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="PurchaseLabel">

    <string name="billInfo" title="Bill Info"/>
    <string name="referenceField1" title="Reference Field 1" required="true"
      namecolumn="true"/>
    <string name="referenceField2" title="Reference Field 2"/>
    <many-to-one name="carrier" ref="com.axelor.apps.sale.db.ShippService"
      title="Carrier"/>
    <many-to-one name="shipPackage" ref="com.axelor.apps.sale.db.ShippPackage"
      title="Packageing"/>
    <string name="labelFileType" title="Label File Type"
      selection="purchase.label.File.type.select"/>
    <string name="phoneNumber" title="Phone"/>
    <string name="emailAddress" title="Email"/>
    <boolean name="isShippingConfirm" title="Is Shipping Confirm"/>

    <string name="trackingNumber" title="Tracking Number"/>
    <string name="lableUrl" large="true" title="URL"/>
    <decimal name="selectedRate" title="Selected Rate"/>

    <one-to-many ref="com.axelor.apps.sale.db.PurchaseLabelRateLine"
      name="purchaseLabelRateLine" title="Shipments" mappedBy="purchaseLable"/>

    <boolean name="isMultipalShipment" title="Is Multipal Shipment"/>
    <one-to-many ref="com.axelor.apps.sale.db.MultiShipmentPackageLine"
      name="multiShipmentPackageLine" title="Multiple Shipment Package" mappedBy="purchaseLable"/>
    <decimal name="totalMultiShipmentCost" title="Total Multi Shipment Cost"/>

    <many-to-one name="customerShipmentInfo"
      ref="com.axelor.apps.sale.db.CustomerShipmentInfo" title="Customer Shipment Info"/>

    <many-to-one name="shippingFromAddress" ref="com.axelor.apps.base.db.Address"
      title="Shipping from address"/>
    <many-to-one name="shippingToAddress" ref="com.axelor.apps.base.db.Address"
      title="Shipping To address"/>

    <decimal name="weight" title="Weight" scale="2" precision="20"/>
    <decimal name="sizeL" title="L" scale="2" precision="20"/>
    <decimal name="sizeW" title="W" scale="2" precision="20"/>
    <decimal name="sizeH" title="H" scale="2" precision="20"/>
    <integer name="totalPackage" title="Total Package"/>

    <many-to-one name="saleOrder" ref="com.axelor.apps.sale.db.SaleOrder"
      title="Sale Order"/>

  </entity>

</domain-models>
