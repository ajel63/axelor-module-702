<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="ShipmentLine">

    <many-to-one name="customer" ref="com.axelor.apps.base.db.Partner" title="Customer"/>
    <string name="trackStatus" title="Track Status" selection="shipment.track.status.select"/>
    <decimal name="rate" title="Rate"/>
    <many-to-one name="carrierService" ref="com.axelor.apps.sale.db.ShippService"
      title="Carrier"/>
    <date name="shippDate" title="Ship Date"/>
    <string name="trackingNumber" title="Tracking Number"/>
    <many-to-one ref="com.axelor.apps.sale.db.SaleOrder" name="saleOrder"
      title="Sale Order"/>
    <string name="carrierServiceType" title="Carrier Service"/>
    <string name="trackingLink" title="Tracking Link"/>
    <string name="lableUrl" large="true" title="URL"/>

  </entity>

</domain-models>
