<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

  <module name="sale" package="com.axelor.apps.sale.db"/>

  <entity name="StripeRefundLine">

    <string name="stripeId" title="Stripe Id"/>
    <string name="refundId" title="Refund Id"/>
    <string name="orderId" title="Order Id"/>
    <string name="productId" title="Product Id"/>
    <string name="customerId" title="Customer Id"/>
    <string name="productName" title="Product Name"/>
    <string name="quantity" title="Quantity"/>
    <string name="totalRefund" title="Total Refund"/>
    <string name="createdAt" title="Created At"/>
    <string name="updatedAt" title="Updated At"/>

    <many-to-one ref="com.axelor.apps.sale.db.SaleOrder" name="saleOrder"
      title="SaleOrder"/>

  </entity>

</domain-models>
