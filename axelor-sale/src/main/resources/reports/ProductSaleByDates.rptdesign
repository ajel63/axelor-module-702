            <xml-property name="queryText"><![CDATA[select 
Product.name as ProdName,
Product.code as ProdCode,
SUM(SaleOrderLine.qty) as Qty,
SUM(SaleOrderLine.ex_tax_total) as ExTaxTotal,
SUM(SaleOrderLine.in_tax_total) as ProdTotal
 
 

from sale_sale_order as SaleOrder
left outer join sale_sale_order_line as SaleOrderLine on (SaleOrder.id = SaleOrderLine.sale_order)
left outer join base_product as Product on (Product.id = SaleOrderLine.product)


@INJECTEDWHERE
GROUP BY ProdCode, ProdName;]]></xml-property>
