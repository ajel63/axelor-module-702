<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_6.1.xsd">

  <grid name="shipp-service-grid" title="ShippService"
    model="com.axelor.apps.sale.db.ShippService">
    <field name="code"/>
    <field name="name"/>
  </grid>

  <form name="shipp-service-form" title="ShippService"
    model="com.axelor.apps.sale.db.ShippService">
    <panel name="servicePanel">
      <field name="code" colSpan="3" required="true"/>
      <field name="name" colSpan="3" required="true"/>
    </panel>
  </form>

</object-views>
